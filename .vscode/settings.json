    ],
    "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
    "code-runner.executorMap": {
        "c": "cd $dir && export mydir='/Users/magicpin/CPP/bin' && /opt/homebrew/bin/gcc-15 -std=c23 $fileName -o $mydir/a.out && $mydir/a.out < $mydir/'#input.txt' > $mydir'/#output.txt'",
        // "cpp": "cd $dir && export mydir='/Users/magicpin/CPP/bin' && /opt/homebrew/bin/g++-15 -std=c++23 $fileName -o $mydir/a.out && $mydir/a.out < $mydir/'#input.txt' > $mydir'/#output.txt'",
        "cpp": "cd $dir && export mydir=$workspaceRoot/bin && /opt/homebrew/bin/g++-15 -std=c++23 $fileName -o $mydir/a.out && $mydir/a.out < $mydir/'#input.txt' > $mydir'/#output.txt'",
    },
    "[code-runner-output]": {
        "editor.fontSize": 14,
        "editor.fontFamily": "Monaco, 'Courier New', monospace",
        "editor.lineHeight": 1.2,
    },
    "search.exclude": {
